  <title>Agenda</title>
  <link rel="icon" type="image/png" href="./static/favicon.png" />
  <link rel="stylesheet" href="static/agenda.css">
  <link rel="stylesheet" href="static/styles.css">
  <link rel="stylesheet" href="./css/bootstrap.min.css">
  <link rel="stylesheet" href="./bicons/bootstrap-icons.css">
  <link rel="stylesheet" href="sweetalert2.min.css">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet" />
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"
    integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.9.2/umd/popper.min.js" integrity="sha512-2rNj2KJ+D8s1ceNasTIex6z4HWyOnEYLVC3FigGOmyQCZc2eBXKgOxQmo3oKLHyfcj53uz4QMsRCWNbLd32Q1g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
  <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
  <script src="./js/bootstrap.min.js"></script>
</head>

<body>
  <div id="custom-target"></div>
  <nav class="navbar navbar-expand-lg navbar-light navbar-style">
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
      aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse justify-content-center" id="navbarNav">
      <ul class="navbar-nav">
        <li class="nav-item text-center active">
          <a class="nav-link" href="/home">Home</a>
        </li>
        <li class="nav-item text-center">
          <a class="nav-link currentNavItem" href="#">Lesrooster</a>
        </li>
        <li class="nav-item text-center">
          <a class="nav-link" href="/producten">Producten</a>
        </li>
        <li class="nav-item text-center">
          <a class="nav-link" href="#">Media</a>
        </li>
        <li class="nav-item text-center">
          <a class="nav-link" href="#">Profiel</a>
        </li>
      </ul>
    </div>
    <div class="dropdown">
      <button class="btn dropdown-toggle bi bi-icons pe-3 me-1" id="dropdownMenuButton" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <i class="bi bi-person"></i>
      </button>
      <div class="dropdown-menu dropdown-menu-right profileItemNav me-4" aria-labelledby="dropdownMenuButton">
        <a class="dropdown-item userNameNav" href="#"></a>
        <a class="dropdown-item userSaldo" href="#"></a>
        <div class="dropdown-divider navSwitch"></div>
      </div>
    </div>
  </nav>
  <div class="main-wrapper container-fluid">
    <div class="row h-100">
      <div class="col-md h-100">
        <div class="row headerRow align-items-center">
          <div class="col-md-12">
            <h1 class="lead text-start m-0 headerText">Lesrooster</h1>
          </div>
        </div>
        <div class="row contentRow">
          <div class="col-md agendaCol">
            <div class="row align-items-center headerSchedule">
              <div class="col text-start">
                
              </div>
              <div class="col text-end">
                <i class="bi bi-chevron-left prevWeek "></i>
              </div>
              <div class="col">
                <p class="lead week text-center m-0">
                </p>
              </div>
              <div class="col text-start">
                <i class="bi bi-chevron-right nextWeek"></i>
              </div>
              <div class="col text-end">
                <div class="row align-items-center">
                  <div class="col-md-9 text-end">
                    
                  </div>
                  <div class="col-md-3">
                    <i class="bi bi-plus-circle hiding addLesson"></i>
                  </div>
                </div>
                
              </div>
            </div>
            <div class="row agendaItemDay d1">
              <div class="col-md p-4">
                <h2 class="lbs maandag lead">Maandag</h2>
                <hr>
                <div id="maandag">
                  <h4 class="lead p-3">Geen lessen</h4>
                </div>
              </div>
            </div>
            <div class="row agendaItemDay d2">
              <div class="col-md p-4">
                <h2 class="lbs dinsdag lead">Dinsdag</h2>
                <hr>
                <div id="dinsdag">
                  <h4 class="lead p-3">Geen lessen</h4>
                </div>
              </div>
            </div>
            <div class="row agendaItemDay d3">
              <div class="col-md p-4">
                <h2 class="lbs woesndag  lead">Woensdag</h2>
                <hr>
                <div id="woensdag">
                  <h4 class="lead p-3">Geen lessen</h4>
                </div>
              </div>
            </div>
            <div class="row agendaItemDay d4">
              <div class="col-md p-4">
                <h2 class="lbs donderdag lead">Donderdag</h2>
                <hr>
                <div id="donderdag">
                  <h4 class="lead p-3">Geen lessen</h4>
                </div>
              </div>
            </div>
            <div class="row agendaItemDay  d5">
              <div class="col-md p-4">
                <h2 class="lbs vrijdag lead">Vrijdag</h2>
                <hr>
                <div id="vrijdag">
                  <h4 class="lead p-3">Geen lessen</h4>
                </div>
              </div>
            </div>
            <div class="row agendaItemDay  d6">
              <div class="col-md p-4">
                <h2 class="lbs zaterdag lead">Zaterdag</h2>
                <hr>
                <div id="zaterdag">
                  <h4 class="lead p-3">Geen lessen</h4>
                </div>
              </div>
            </div>
            <div class="row agendaItemDay d0">
              <div class="col-md p-4">
                <h2 class="lbs zondag lead">Zondag</h2>
                <hr>
                <div id="zondag">
                  <h4 class="lead p-3">Geen lessen</h4>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  </div>

  
  
  <script src="sweetalert"></script>
  <script src="static/utils.js"></script>
  <script src="static/agendaTemplateItems.js"></script>
  <script src="static/agenda.js"></script>
  <script src="static/apiCaller.js"></script>
</body>

</html>